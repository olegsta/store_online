<body>
<% title "Sign up" %>
<div class="row">
  <div class="col-md-3">
  </div>
  <div class="col-md-6">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="form-group">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, size: "60" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", size: "60" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br/>
    <%= f.password_field :password_confirmation, autocomplete: "off", size: "60" %>
  </div>


  <div class="actions">
    <%= f.button "Sign up", data: {disable_with: "<i class='fa fa-cog fa-spin'></i> Sign up..."}  %>
  </div>
<% end %>

<%= render "devise/google"%>
</div>
  <div class="col-md-3">
  </div>
</div>
</body>